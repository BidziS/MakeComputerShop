<div class="row">
    <img src="@Model.ImageUrl"
         alt="boohoo" class="image-details img-responsive"/>
</div>
<div class="col-lg-offset-2 col-lg-8 col-lg-offset-2 col-md-offset-1 col-md-10 col-md-offset-1">
    <div class="table-products">
        <table class="table table-bordered table-hover table-responsive table-striped table-condensed">
            <thead></thead>
            <tbody>
            <tr>
                <td>Nazwa:</td>
                <td>@Model.Name</td>
            </tr>
            <tr>
                <td>
                    Częstotliwość: 
                </td>
                <td>@Model.Frequency MHz</td>
            </tr>
            <tr>
                <td>Ilość rdzeni: </td>
                <td>@Model.NumberOfCores</td>
            </tr>
            <tr>
                <td>Ilość wątków: </td>
                <td>@Model.NumberOfThreads</td>
            </tr>
            <tr>
                <td>Cache: </td>
                <td>@Model.Cache</td>
            </tr>
            <tr>
                <td>Socket: </td>
                <td>@Model.Socket.Name</td>
            </tr>
            <tr>
                <td>Producent: </td>
                <td>@Model.Producent.Name</td>
            </tr>
            <tr>
                <td>Cena: </td>
                <td>@Model.Price zl</td>
            </tr>
            </tbody>
        </table>
    </div>
</div>                                                        
